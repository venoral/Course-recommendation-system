<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<filter>
	    <filter-name>CORSFilter</filter-name>
	    <filter-class>filter.CORSFilter</filter-class>
	</filter>
 
	<filter-mapping>
	    <filter-name>CORSFilter</filter-name>
	    <url-pattern>/*</url-pattern>
	</filter-mapping>
	
	<servlet>
		<servlet-name>CourseServlet</servlet-name>
		<servlet-class>servlet.CourseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CourseServlet</servlet-name>
		<url-pattern>/servlet/CourseServlet</url-pattern>
	</servlet-mapping>
</web-app>